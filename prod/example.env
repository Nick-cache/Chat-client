BALANCER_NAME=balancer
BALANCER_HTTP_PORT=81

FRONTEND_DOMAIN_LINK="https://${DOMAIN}"
FRONTEND_DEV=false

LMS_CORS=["${BACKEND_SERVICE}", "${FRONTEND_DOMAIN_LINK}"]
