LMS_LOCAL_PORT=1234
LMS_PORT=3000
LMS_DOMAIN_LINK="https://${DOMAIN}/api/lms"
LMS_SERVICE="http://${LMS_NAME}:${LMS_PORT}"
LMS_SOCKET="https://${DOMAIN}"

